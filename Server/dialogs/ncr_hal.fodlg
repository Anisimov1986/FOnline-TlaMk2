4 2020 D _param p ST_INTELLECT > 3 D _var p q_ncr_redding_caravan_escort = 0 #
3 2030 D _param p ST_INTELLECT > 3 #
3 2040 D _param p ST_INTELLECT < 4 #
6 2050 D _script caravan@d_HasPlayerRequest 0  D _var p q_ncr_redding_caravan_escort > 0 #
